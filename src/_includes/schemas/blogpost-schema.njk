<script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "BlogPosting",
    "mainEntityOfPage": {
      "@type": "WebPage",
      "@id": "https://cybercultural.com{{ page.url }}"
    },
    "isPartOf": {
      "@id": "{{ meta.url }}#website"
    },
    "@id": "https://cybercultural.com{{ page.url }}",
    "headline": "{{ title or meta.siteName }}",
    "description": "{{ discover.description or description or meta.siteDescription }}",
    "image": "{{ meta.url }}{% if featureImage %}{{ featureImage }}{% else %}{{ meta.opengraph_default }}{% endif %}",
    "inLanguage": "{{ meta.locale }}",
    "publisher": {
      "@type": "{{ meta.siteType }}",
      "name": "{{ meta.organization.name }}",
      "url": "{{ meta.url }}"
    },
    "author": {
      "@type": "Person",
      "name": "{{ meta.author.name }}",
      "url": "https://cybercultural.com/about/"
    },
    "datePublished": "{{ date | toIsoString }}"
  }
</script>